{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "airbnb", "airbnb/hooks", "eslint:recommended", "prettier"],
  "plugins": ["react"],
  "rules": {
    "global-require": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": "off",
    "jsx-a11y/anchor-is-valid": "off"
  }
}
