{
  "name": "laganoo-frontapp-test-2",
  "version": "0.1.0",
  "description": "SPA app built with ReactJS and TailwindCss. Sidebar and private office",
  "keywords": [
    "react",
    "tailwindcss",
    "spa"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.3",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
